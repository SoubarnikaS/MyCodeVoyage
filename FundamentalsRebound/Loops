import java.util.Scanner;
public class series{
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        int num=s.nextInt();
        for(int i=0;i<num;i++){
            int a=s.nextInt();
            int b=s.nextInt();
            int n=s.nextInt();
            for(int j=0;j<n;j++){
               System.out.print((a+=(Math.pow(2,j)*b))+" ");   
            }
            System.out.println();
        }
    }
}
